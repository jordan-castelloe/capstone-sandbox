{{#each results}}
<article class="search-result card" id="{{this.place_id}}">
    <div class="card-body">
        <h4 class="result-name card-title">{{this.name}}</h3>
        <p class="result-address">{{this.formatted_address}}</p>
        <button class="btn btn-secondary addToTrip">Add To Trip</button>
    </div>
</article>
{{/each}}